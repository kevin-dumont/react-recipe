{
  "name": "new",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "./dist/server.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf && rm -rf ./dist/*",
    "tscheck": "tsc --noEmit -p ./tsconfig.json && tsc --noEmit -p ./tsconfig-server.json",
    "lint": "eslint -c tslint.json '{definitions,src}/**/*.{ts,tsx}'",
    "build-server": "yarn clean && tsc -p ./tsconfig-server.json && node dist/server/index.js",
    "build-client": "cross-env NODE_ENV=production webpack -p --progress",
    "start-client": "cross-env NODE_ENV=development && yarn run clean && webpack --debug --watch",
    "prettier": "prettier src/**/*",
    "start-server-old": "concurrently --kill-others \"tsc -p ./tsconfig-server.json -w\" \"nodemon dist/server/index.js\"",
    "start-server": "nodemon dist/server.js",
    "dev": "yarn clean && webpack --watch"
  },
  "dependencies": {
    "@types/express-session": "^1.15.16",
    "@types/node": "^12.12.12",
    "@types/request": "^2.48.3",
    "express": "^4.17.1",
    "jsx-to-string": "1.4.0",
    "object-editor-react": "1.1.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.2",
    "@types/styled-components": "4.4.0",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "nodemon": "^2.0.1",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "printWidth": 120,
    "trailingComma": "all"
  }
}
